{
    "manifest_version": 2,

    "name": "beeopen",
    "description": ""
    "version": "1.0",

    "icons": { 
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png" 
    },
          
    "permissions": [
        "storage"
    ],
  
    "content_scripts": [
        {
            "matches": ["http://www.beestar.org/*"],
            "css": ["beeopen.css"],
            "js": ["jquery-3.2.1.min.js", "beeopen.js"]
        }
    ]
}
